<%= semantic_form_for resource ,:remote => true,:html => {:id => "cash_payment_list_form",:class => "cash_payment_list bill_selector"} do |f|%>
  <%=f.error_messages %>
  <%= f.inputs do %>
    <%= f.input :org_id,:label =>"分理处/分公司" ,:hint => "可录入拼音简写查询发货地(例如:安阳,可录入ay)",:as => :select,:collection => branches_for_select,:include_blank => false,:input_html => {:class => "select_org",:id => "from_org_id"} %>
    <%= f.input :note,:as => :string,:input_html => {:size => 60} %>
  <% end %>
<% end%>
