<%= render "form_before" %>
<%= semantic_form_for resource ,:remote => true,:html => {:id => "transit_deliver_form"} do |f|%>
  <%=f.error_messages %>
  <%= f.inputs do %>
    <%= f.input :bill_date,:label =>"中转日期",:as => :string,:input_html =>{:readonly => true,:size => 20,:class => 'datepicker'} %>
    <%= f.input :org_id,:as => :select,:label=>"中转部门",:collection => current_ability_orgs_for_select,:include_blank => false,:input_html => {:class => "select_org",:id => "transit_org_id"} %>
    <%= f.input :transit_hand_fee,:label => "中转手续费" %>
    <%= f.input :note,:as => :string,:label=> "备注",:input_html => {:size => 60} %>
  <%end%>
<% end%>
