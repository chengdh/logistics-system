<%= semantic_form_for resource ,:remote => true,:html => {:id => "transfer_payment_list_form",:class => "transfer_payment_list bill_selector"} do |f|%>
  <%=f.error_messages %>
  <%= f.inputs do %>
    <%= f.input :org_id,:as => :select,:collection => current_ability_orgs_for_select,:include_blank => false,:input_html => {:class => "select_org",:id => :from_org_id} %>
    <%= f.input :bank_id,:label =>"银行" ,:hint => "请选择要转账的银行",:as => :select,:collection => banks_for_select,:include_blank => false,:input_html => {:id => "bank_id"} %>
    <%= f.input :note,:as => :string,:input_html => {:size => 60} %>
  <% end %>
<% end%>
